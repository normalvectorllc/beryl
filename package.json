{
    "name": "task-management-system",
    "version": "1.0.0",
    "private": true,
    "description": "A sophisticated task management system with AI-powered task breakdown functionality",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "seed": "turbo run seed",
        "migrate": "turbo run migrate"
