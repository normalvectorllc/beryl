    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "prettier": "^3.0.0",
        "turbo": "^1.10.12",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "npm@8.19.4",
    "dependencies": {
        "dotenv": "^16.4.7",
        "openai": "^4.86.2"
